# API Keys - Replace with your actual API keys
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Veo 3 Settings
VEO_MODEL=veo-3.0-fast-generate-preview  # Options: veo-3.0-fast-generate-preview, veo-3.0-generate-preview
VEO_MAX_WAIT_TIME=600  # Maximum wait time for video generation (seconds)

# Video Settings
VIDEO_PRIVACY_STATUS=public  # Options: public, private, unlisted
VIDEO_CATEGORY_ID=22  # 22 = People & Blogs, 24 = Entertainment

# Logging
LOG_LEVEL=INFO